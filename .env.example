# Copy this file to .env.local and fill in real values for Firebase mode
# All VITE_ prefixed vars are exposed to the browser via import.meta.env

# Set to "true" to use mock data (no Firebase required)
# Set to "false" to use real Firebase
VITE_USE_MOCK=true

# Firebase project config (only needed when VITE_USE_MOCK=false)
VITE_FIREBASE_API_KEY=
VITE_FIREBASE_AUTH_DOMAIN=
VITE_FIREBASE_PROJECT_ID=
VITE_FIREBASE_STORAGE_BUCKET=
VITE_FIREBASE_MESSAGING_SENDER_ID=
VITE_FIREBASE_APP_ID=
